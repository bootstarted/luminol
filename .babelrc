{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "8"
      },
      "modules": "commonjs",
      "shippedProposals": true
    }],
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-graphql-tag",
    ["module-resolver", {
      "alias": {"": "./src", "#test": "./test"},
    }],
  ]
}
